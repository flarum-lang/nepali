{
	"name": "flarum-lang/nepali",
	"type": "flarum-extension",
	"description": "Nepali language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/nepali",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"ne-locale",
		"nepali-locale",
		"nepali"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/nepali/issues",
		"source": "https://github.com/flarum-lang/nepali"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Nepali",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "ne",
			"title": "Nepali"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
